{
	"compilerOptions": {
		"lib": [
			"es2020"
		],
		"module": "commonjs",
		"target": "es2020",
		"sourceMap": true,
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"outDir": "./dist"
	},
	"exclude": [
		"node_modules",
		"client"
	]
}
